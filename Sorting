class Sorting 
{
public static void main(String arg[])
{
int a[]={2,4,5,7,8}
int temp=0;

for(i=0;i<arr.length;i++)
{
for(int j=i+1;j<arr.length;j++)
{
if(arr[i]>arr[j])
{
temp=arr[i];
arr[i]=arr[j];
arr[j]=temp;
}
}
}
System.out.println("element in a array sorted");
for(int i=0;i<arr.length;i++)
{
System.out,println(arr[i]);
}
}
} 
