import java.util.*;
class Voting
{
 public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int age,years;
System.out.println("enter the age");
age=sc.nextInt();
years=18-age;
if(age>=18)
{
System.out.println("person can vote");
}
else
{
System.out.println("person can't vote and have to wait "+years+"years");
}
}
}
