 class Student {
    int registerNumber;
    char name;
    double marks;

Student(int registerNumber, String name, double marks) {
        this.registerNumber = registerNumber;
        this.name = name;
        this.marks = marks;
    }
 void displayDetails() {
        System.out.println("Register Number: " + registerNumber);
        System.out.println("Name: " + name);
        System.out.println("Marks: " + marks);
    }
 static void main(String[] args) {
        Student student = new Student(12345, "John Doe", 95.5);
        student.displayDetails();
    }
}
