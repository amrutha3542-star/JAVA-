import java.util.*;
class Array 
{
public static void main(String arg[])
{
int a[]=new int[5];
Scanner sc=new Scanner(System.in);
System.out.println("enter the array values:");
for(int i=0;i<=4;i++)
{
a[i]=sc.nextInt();
}
System.out.println("the array elements are in reverse order");
for(int i=4;i>=0;i--)
{
System.out.println(a[i]);
}
}
}
