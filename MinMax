class MinMAx
{
Public static void main(String arg[])
{
int a[]={23,56,78,34,65};
int min = numbers[0];
int max = numbers[0];
for(int i=1;i<a.length;i++) {
if(numbers[i] < min) {
min = numbers[i];
}
if(numbers[i] > max) {
max = numbers[i];
}
}
System.out.println("Minimum Value: " + min);
System.out.println("maximum Value: " + max);
}
}
