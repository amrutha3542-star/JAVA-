import java.util.*;
class Display
{
public static void main(String arg[])
{
int a[]={10,20,30,40};
System.out.println("the array values are");
for(int i=0;i<0.length;i++)
{
System.out.println(a[i]);
}
System.out.println("the array values are in reverse order");
for(int i=0;i>0.length;i--)
{
System.out.println(a[i]);
}
}
}
