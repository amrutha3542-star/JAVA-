class Mul
{
int n,m,i;

void multi()
{
for(i=1;i<=10;i++)
{
System.out.println("11x"+i+"="+11*i);
}
}
void multi(int x)
{
m=x;
for(i=1;i<=10;i++)
{
System.out.println(m+"x"+i+"="+m*i);
}
}
}
class MulTest
{
public static void main(String arg[])
{
Mul myMul=new Mul();
myMul.multi();
myMul.multi(13);
}
}



